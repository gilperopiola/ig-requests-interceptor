{
  "name": "ig-requests-interceptor",
  "version": "1.1",
  "manifest_version": 2,
  "content_scripts": [{
    "js": [ "jquery-3.6.0.min.js", "interceptor.js"],
    "run_at": "document_start",
    "matches": ["https://*.instagram.com/*"]
  }],
  "permissions": ["background", "tabs", "webRequest", "webRequestBlocking", "*://*/*"]
}